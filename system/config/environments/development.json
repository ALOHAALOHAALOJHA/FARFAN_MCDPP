{
  "_metadata": {
    "version": "1.0.0",
    "description": "Development environment configuration - optimized for fast iteration and debugging",
    "last_updated": "2025-01-10",
    "environment": "development",
    "implementation_wave": "GOVERNANCE_WAVE_2024_12_07",
    "wave_label": "CONFIG_GOVERNANCE_STRICT_FOLDERIZATION",
    "maintainer": "DevOps Team"
  },
  "api": {
    "host": "127.0.0.1",
    "port": 5000,
    "debug": true,
    "workers": 1,
    "timeout": 600,
    "max_content_length": 52428800,
    "reload": true
  },
  "database": {
    "connection_pool_size": 5,
    "connection_timeout": 10,
    "query_timeout": 30
  },
  "executor": {
    "timeout_s": 60.0,
    "retry": 2,
    "max_tokens": 1024,
    "temperature": 0.8,
    "seed": 42
  },
  "logging": {
    "level": "DEBUG",
    "format": "text",
    "output": "console",
    "retention_days": 7
  },
  "security": {
    "enable_auth": false,
    "jwt_expiration": 86400,
    "rate_limiting": false,
    "max_requests_per_minute": 1000
  },
  "cache": {
    "enabled": false,
    "ttl": 60,
    "max_size_mb": 256
  },
  "resources": {
    "max_workers": 2,
    "memory_limit_mb": 2048,
    "enable_profiling": true
  },
  "monitoring": {
    "enable_metrics": true,
    "enable_tracing": true,
    "sample_rate": 1.0
  },
  "_notes": [
    "Development environment uses lower resource limits for local testing",
    "Higher temperature for exploring LLM behavior",
    "Reduced timeout for faster failure feedback",
    "Full tracing enabled for debugging"
  ]
}
