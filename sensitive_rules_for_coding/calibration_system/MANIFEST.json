  "features": {
    "weighted_aggregation": {
      "formula": "@b = 0.40 × b_theory + 0.35 × b_impl + 0.25 × b_deploy",
      "weights": {
        "b_theory": 0.40,
        "b_impl": 0.35,
        "b_deploy": 0.25
      }
    },
    "status_management": {
      "excluded": "Methods blacklisted from production (score = 0.0)",
      "pending": "Methods awaiting calibration (uses fallbacks)",
      "computed": "Methods with evidence-based scores",
      "unknown": "Methods without calibration data (uses defaults)"
    },
    "fallback_mechanism": {
      "priority": [
        "Excluded → 0.0",
        "Pending + explicit fallback → fallback value",
        "Pending + role → role default",
        "Pending → 0.6",
        "Computed → computed score",
        "Unknown + explicit fallback → fallback value",
        "Unknown + role → role default",
        "Unknown → 0.6"
      ]
    },
    "role_based_defaults": {
      "SCORE_Q": 0.7,
      "AGGREGATE": 0.7,
      "META_TOOL": 0.7,
      "INGEST_PDM": 0.6,
      "STRUCTURE": 0.6,
