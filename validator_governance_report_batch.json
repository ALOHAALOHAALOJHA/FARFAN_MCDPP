{
  "meta_rule": {
    "id": "NO_EPISTEMIC_FLATTENING",
    "statement": "The validator SHALL NOT reduce epistemological diversity across contracts through automatic corrections.",
    "enforced_by": [
      "N1_protection_guard",
      "gate_logic_guard",
      "asymmetry_guard",
      "argumentative_role_guard"
    ],
    "violation_action": "HARD_FAIL"
  },
