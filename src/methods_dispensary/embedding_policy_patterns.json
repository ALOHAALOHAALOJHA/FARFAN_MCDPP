{
  "numerical_patterns": [
    {
      "name": "mil_millones",
      "regex": "\\b\\d{1,3}(?:[.,]\\d{3})*(?:[.,]\\d+)?\\s*mil\\s+millones?\\b"
    },
    {
      "name": "millones",
      "regex": "\\b\\d{1,3}(?:[.,]\\d{3})*(?:[.,]\\d+)?\\s*(?:millones?|mill[oรณ]n)\\b"
    },
    {
      "name": "currency",
      "regex": "\\$\\s*\\d{1,3}(?:[.,]\\d{3})*(?:[.,]\\d+)?"
    },
    {
      "name": "percentage",
      "regex": "\\b\\d+(?:[.,]\\d+)?\\s*%\\b"
    },
    {
      "name": "people",
      "regex": "\\b\\d+(?:[.,]\\d+)?\\s*(?:personas|beneficiarios|habitantes)\\b"
    }
  ]
}

